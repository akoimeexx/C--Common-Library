    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:__UNIT__"
    xmlns:lc="clr-namespace:Common.Converters;assembly=Common.Library" 
    xmlns:extensions="clr-namespace:Common.Extensions.Wpf;assembly=Common.Library"
    xmlns:res="clr-namespace:Common;assembly=Common.Library"
    mc:Ignorable="d"
    Title="Common.Libraries Test Window" MinHeight="193" MinWidth="171" 
    Height="640" Width="1133" ResizeMode="CanResizeWithGrip" 
    WindowStartupLocation="CenterScreen" Icon="CommonLibrary.resource.ico">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Common.Library;component/brushes.backgrounds.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Common.Library;component/brushes.borders.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Common.Library;component/checkbox.disabled.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Common.Library;component/checkbox.lockbox.xaml" />
                <!--<ResourceDictionary Source="pack://application:,,,/Common.Library;component/textbox.placeholders.xaml" />-->
            </ResourceDictionary.MergedDictionaries>
            <!-- Common Converters -->
            <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
            <lc:AggregateConverters x:Key="InverseBooleanToVisibility">
                <lc:BooleanToInverseConverter />
                <BooleanToVisibilityConverter />
            </lc:AggregateConverters>
            <lc:IconToImageSourceConverter x:Key="IconToImageSource" />
            <!-- /Common Converters -->
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Menu Height="18" VerticalAlignment="Top">
            <MenuItem Header="_Dialogs">
                <MenuItem x:Name="menuColorDialog" Header="ColorDialog()" 
                Click="MenuColorDialog_Click">
                    <MenuItem.Icon>
                        <Image Height="16" Width="16"
                            Source="{Binding Source={x:Static 
                                res:Resources_Common.ColorDialog_Icon}, 
                                Converter={StaticResource IconToImageSource}}"/>
                    </MenuItem.Icon>
                </MenuItem>
                <MenuItem Header="FontDialog()" Click="MenuFontDialog_Click">
                    <MenuItem.Icon>
                        <Image Height="16" Width="16"
                            Source="{Binding Source={x:Static 
                                res:Resources_Common.FontDialog_Icon}, 
                                Converter={StaticResource IconToImageSource}}"/>
                    </MenuItem.Icon>
                </MenuItem>
                <Separator />
                <MenuItem Header="OpenDialog()" Click="MenuOpenDialog_Click" />
                <MenuItem Header="SaveDialog()" Click="MenuSaveDialog_Click" />
                <MenuItem Header="PathDialog()" Click="MenuPathDialog_Click" />
                <Separator />
                <MenuItem Header="PromptDialog()" Click="MenuPromptDialog_Click" />
            </MenuItem>
            <MenuItem Header="Extensions">
                <MenuItem Header="Primitive Extensions">
                    <MenuItem Header="Objects">
                        <MenuItem Header="Dump()" 
                            Click="MenuDumpObject_Click" />
                    </MenuItem>
                    <MenuItem Header="Strings">
                        <MenuItem Header="CollapseWhitespace()" 
                            Click="MenuCollapseWhitespace_Click" />
                    </MenuItem>
                </MenuItem>
                <MenuItem Header="Wpf Extensions">
                    <MenuItem Header="Window Extensions">
                        <MenuItem Header="DisableHitTesting()" 
                            Click="MenuDisableHitTesting_Click" />
                        <Separator />
                        <MenuItem Header="DisableMaximize()" 
                            Click="MenuDisableMaximize_Click" />
                        <MenuItem Header="EnableMaximize()" 
                            Click="MenuEnableMaximize_Click" />
                    </MenuItem>
                </MenuItem>
            </MenuItem>
            <MenuItem Header="_Generics">
                <MenuItem Header="NotificationIcon()" 
                    Click="MenuCreateIcon_Click" />
            </MenuItem>
            <MenuItem Header="_Localization">
                <MenuItem Header="FullName.ToString()" 
                    Click="MenuFullname_Click" />
            </MenuItem>
        </Menu>
